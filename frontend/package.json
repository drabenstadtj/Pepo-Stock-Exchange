{
  "dependencies": {
    "axios": "^1.7.2",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "pug": "^3.0.3"
  },
  "scripts": {
    "build-css": "sass src/scss/styles.scss public/stylesheets/style.css",
    "build": "webpack --config webpack.config.js",
    "start-frontend": "webpack --config webpack.config.js --watch",
    "start-backend": "cd ../backend && python run.py",
    "run": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "start-prod": "npm run build && concurrently \"npm run start-frontend\" \"npm run start-backend\""
},
  "devDependencies": {
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
